{
  "version": "v4",
  "classes": [
    "01",
    "03",
    "04",
    "05",
    "06"
  ],
  "batch_size": 32,
  "epochs": 50,
  "learning_rate": 0.0005,
  "num_filters": 128,
  "kernel_size": 3,
  "number_of_blocks": 4,
  "residuals_per_block": 10,
  "downsample_factor": 1,
  "pool_type": "avg",
  "dropout": 0.3,
  "weight_decay": 0.0001,
  "num_workers": 4,
  "split_seed": 42,
  "global_seed": 4,
  "dataset": {
    "root": "./DatasetPDT",
    "subdir": "avt/Processed",
    "key": "data_last5",
    "end_t": 64000,
    "window": 2048,
    "no_overlap": true,
    "normalize_train_stats_only": true,
    "augmentation": {
      "aug_enable": true,
      "p_shift": 0.7,
      "shift_max": 128,
      "p_reverse": 0.2,
      "p_scale": 0.5,
      "scale_min": 0.9,
      "scale_max": 1.1,
      "p_noise": 0.8,
      "noise_k_min": 0.03,
      "noise_k_max": 0.07,
      "noise_bias_power": 2.0
    },
    "val_clean_only": true
  }
}